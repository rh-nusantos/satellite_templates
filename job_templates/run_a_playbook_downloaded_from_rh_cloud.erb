<%#
name: Run a playbook downloaded from RH cloud
snippet: false
template_inputs:
- name: playbook_url
  required: true
  input_type: user
  description: URL of the playbook to run
  advanced: false
  value_type: plain
  hidden_value: false
- name: organization_id
  required: true
  input_type: user
  description: Id of the organization that owns the playbook
  advanced: false
  value_type: plain
  hidden_value: false
- name: report_url
  required: true
  input_type: user
  description: URL for calling the report callback
  advanced: false
  value_type: plain
  hidden_value: false
- name: correlation_id
  required: true
  input_type: user
  description: correlation ID used to identify the original calling message
  advanced: false
  value_type: plain
  hidden_value: false
- name: report_interval
  required: true
  input_type: user
  description: Reporting interval for the task. A report will be generated every report_interval
    seconds.
  advanced: false
  value_type: plain
  hidden_value: false
model: JobTemplate
job_category: Red Hat Insights
description_format: Playbook generated by RH Cloud
provider_type: Ansible
kind: job_template
organizations:
- Home
locations:
- Brussels
%>

<%= download_rh_playbook(input('playbook_url'), input('organization_id')) %>